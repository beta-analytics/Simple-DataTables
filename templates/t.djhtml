<table id="table" class="table table-hover">
    <thead>
        <tr>
            {% for h in headers %}
                <th>{{h}}</th>
            {% endfor %}
        </tr>
    </thead>
    <tbody>
        {% for row in query %}
        <tr>
            {% for data in row %}
            <td>
                {{ data }}
            </td>
            {% endfor %}
        </tr>
        {% endfor %}
    </tbody>
</table>